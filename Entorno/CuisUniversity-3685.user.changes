!Fraccion methodsFor: 'arithmetic operations' stamp: 'ic 4/10/2019 20:59:37' prior: 50718563!
* aMultiplier 
	
	"^(numerator * aMultiplier numerator) / (denominator * aMultiplier denominator)"
	
	aMultiplier class = Entero ifTrue: [^aMultiplier * self].
	^(numerator * aMultiplier numerator) / (denominator * aMultiplier denominator).

	! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'lf 4/8/2019 21:57:28' prior: 50718413!
+ anAdder 
	
	| newNumerator newDenominator |

	anAdder class = Entero ifTrue: [^anAdder + self].
		
	newNumerator := (numerator * anAdder denominator) + (denominator * anAdder numerator).
	newDenominator := denominator * anAdder denominator.
	
	^newNumerator / newDenominator ! !
!Fraccion methodsFor: 'arithmetic operations' stamp: 'ic 4/10/2019 22:20:44' prior: 50718959!
- aSubtrahend 
	
	| newNumerator newDenominator |
	
	"newNumerator := (numerator * aSubtrahend denominator) - (denominator * aSubtrahend numerator).
	newDenominator := denominator * aSubtrahend denominator.
	
	^newNumerator / newDenominator "
	
	
	aSubtrahend class  = Entero ifTrue: [^Fraccion with: numerator - (aSubtrahend * denominator) over: denominator ].
	newNumerator := (numerator * aSubtrahend denominator) - (denominator * aSubtrahend numerator).
	newDenominator := denominator * aSubtrahend denominator.
	
	^newNumerator / newDenominator! !
